maintainer: "The Gillian Team"
authors: "The Gillian Team"
license: "BSD-3-Clause"
homepage: "https://github.com/GillianPlatform/Gillian"
bug-reports: "https://github.com/GillianPlatform/Gillian/issues"
depends: [
  "alcotest" {= "1.0.1"}
  "angstrom" {= "0.16.0"}
  "angstrom-lwt-unix" {= "0.16.0"}
  "astring" {= "0.8.5"}
  "base" {= "v0.16.3"}
  "base-bigarray" {= "base"}
  "base-bytes" {= "base"}
  "base-domains" {= "base"}
  "base-nnp" {= "base"}
  "base-threads" {= "base"}
  "base-unix" {= "base"}
  "bigstringaf" {= "0.9.1"}
  "camlp-streams" {= "5.0.1" & with-doc}
  "cmdliner" {= "1.2.0"}
  "conf-bash" {= "1"}
  "conf-c++" {= "1.0"}
  "conf-gmp" {= "4"}
  "conf-pkg-config" {= "3"}
  "conf-python-3" {= "9.0.0"}
  "conf-sqlite3" {= "1"}
  "cppo" {= "1.6.9"}
  "crunch" {= "3.3.1" & with-doc}
  "csexp" {= "1.5.2"}
  "dap" {= "1.0.6"}
  "dune" {= "3.15.2"}
  "dune-configurator" {= "3.15.2"}
  "fmt" {= "0.9.0"}
  "fpath" {= "0.7.3" & with-doc}
  "host-arch-x86_64" {= "1"}
  "host-system-other" {= "1"}
  "jane-street-headers" {= "v0.16.0"}
  "jst-config" {= "v0.16.0"}
  "logs" {= "0.7.0"}
  "lwt" {= "5.7.0"}
  "lwt_ppx" {= "2.1.0"}
  "lwt_react" {= "1.2.0"}
  "memtrace" {= "0.2.3"}
  "menhir" {= "20231231"}
  "menhirCST" {= "20231231"}
  "menhirLib" {= "20231231"}
  "menhirSdk" {= "20231231"}
