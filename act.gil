import "wisl_pointer_arith.gil", "wisl_core.gil";


bispec read (x,
y) : [[  (x == #x) * (y == #y)  ]]
spec read(x, y)
  [[  (x == #x) * (y == #y) * (x == 5i)  ]]
  [[  (ret == y)  ]]
  normal
proc read(x, y) {
             goto [((typeOf y) = List)] continue0 fail0;
  fail0:     fail [InvalidPointer]();
  continue0: gvar0 := [getcell](l-nth(y, 0i), l-nth(y, 1i));
             r := l-nth(gvar0, 2i);
             ret := r;
             return
};
